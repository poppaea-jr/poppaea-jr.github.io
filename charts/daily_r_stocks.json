{
    "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
    "data": {
      "url": "https://raw.githubusercontent.com/poppaea-jr/poppaea-jr.github.io/main/daily_stocks_polarity.csv",
      "format": {"type": "csv"}
    },
    "title": {
      "text": "Online Sentiment in the r/Stocks SubReddit",
      "subtitle": "Period from 17/12 to 07/01. Using TextBlob. Source: PRAW",
      "subtitleFontStyle": "italic",
      "subtitleFontSize": 10,
      "anchor": "start",
      "color": "black"
    },
    "width": 300,
    "height": 300,
    "mark": {
      "type": "line",
      "point": false,
      "color": "DarkCyan",
      "strokeWidth": 2,
      "opacity": 1
    },
    "encoding": {
      "x": {"field": "numdate", "type": "temporal", "title": null},
      "y": {"field": "polarity", "type": "quantitative", "title": null}
    }
  }