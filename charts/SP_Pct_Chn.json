{
    "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
    "data": {
      "url": "https://raw.githubusercontent.com/poppaea-jr/poppaea-jr.github.io/main/SP_Pct_Chn.csv",
      "format": {"type": "csv"}
    },
    "title": {
      "text": "Variations in S&P500 Closing Price",
      "subtitle": "Source: Nasdaq",
      "subtitleFontStyle": "italic",
      "subtitleFontSize": 10,
      "anchor": "start",
      "color": "black"
    },
    "width": 300,
    "height": 300,
    "mark": {
      "type": "line",
      "point": false,
      "color": "DarkCyan",
      "strokeWidth": 2,
      "opacity": 1
    },
    "encoding": {
      "x": {"field": "Date", "type": "temporal", "title": null, "axis": {"grid": false}},
      "y": {"field": "Pct_Change", "type": "quantitative", "title": null}
    }
  }