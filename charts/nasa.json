{
    "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
    "data": {
      "url": "https://raw.githubusercontent.com/poppaea-jr/poppaea-jr.github.io/main/senate_wind.csv"
    },
    "title": {
      "text": "Wind Speed at 10m - Senate House",
      "subtitle": "Source: NASA Power REST API",
      "subtitleFontStyle": "italic",
      "subtitleFontSize": 10,
      "anchor": "start",
      "color": "black"
    },
    "width": 300,
    "height": 300,
    "mark": {"type": "line", "color": "DarkCyan", "strokeWidth": 2, "opacity": 1},
    "encoding": {
      "x": {"field": "date", "type": "temporal", "title": null},
      "y": {"field": "wind", "type": "quantitative", "title": null}
    }
  }